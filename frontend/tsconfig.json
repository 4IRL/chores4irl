{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "jsx": "react-jsx",
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "paths": {
            "@assets/*": ["./assets/*"],
            "@customTypes/*": ["../types/*"],
            "@src/*": ["./*"],
        },
        "target": "ES2020",
    },
    "include": [
        "src"
    ]
}